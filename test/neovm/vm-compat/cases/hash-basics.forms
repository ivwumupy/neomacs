; hash-table basic behavior and error-path compatibility.
(let ((h (make-hash-table :test 'eq)))
  (puthash 'a 1 h)
  (gethash 'a h))
(let ((h (make-hash-table :test 'equal)))
  (puthash "a" 1 h)
  (gethash "a" h))
(let ((h (make-hash-table)))
  (gethash 'missing h 'fallback))
(let ((h (make-hash-table)))
  (puthash 'a 1 h)
  (hash-table-count h))
(let ((h (make-hash-table)))
  (puthash 'a 1 h)
  (remhash 'a h)
  (hash-table-count h))
(let ((h (make-hash-table)))
  (puthash 'a 1 h)
  (clrhash h)
  (hash-table-count h))
(condition-case err (puthash 'a 1 7) (error (car err)))
(condition-case err (gethash 'a 7) (error (car err)))
(condition-case err (hash-table-count 7) (error (car err)))
