(let ((orig (symbol-function :k)))
  (unwind-protect
      (progn
        (fset :k 'car)
        (funcall :k '(1 2)))
    (fset :k orig)))
(let ((orig (symbol-function :k)))
  (unwind-protect
      (progn
        (fset :k 'car)
        (apply :k '((1 2))))
    (fset :k orig)))
(let ((orig (symbol-function :k)))
  (unwind-protect
      (progn
        (fset :k nil)
        (fboundp :k))
    (fset :k orig)))
(let ((orig (symbol-function :k)))
  (unwind-protect
      (progn
        (fset :k nil)
        (condition-case err (funcall :k) (error err)))
    (fset :k orig)))
(let ((orig (symbol-function :k)))
