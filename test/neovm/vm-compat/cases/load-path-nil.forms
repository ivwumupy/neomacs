; nil in load-path should resolve to default-directory.
(progn
  (setq vm-loadpath-nil-dir (expand-file-name "load-path-nil-fixtures" (file-name-directory (getenv "NEOVM_FORMS_FILE"))))
  'ok)

(setq vm-loadpath-nil-hit nil)
(setq default-directory (file-name-as-directory vm-loadpath-nil-dir))
(setq load-path '(nil))
(load "vm-loadpath-nil-probe" nil 'nomessage)
vm-loadpath-nil-hit
