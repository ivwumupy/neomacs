  (error err))

(condition-case err
    (with-temp-buffer
      (insert "Abc Def")
      (goto-char 1)
      (call-interactively 'downcase-word)
      (buffer-string))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 1)
      (command-execute 'transpose-lines)
      (let ((s (buffer-string)))
        (and (= (length s) 5)
             (= (aref s 0) 10)
             (= (aref s 1) ?a)
             (= (aref s 2) ?b)
             (= (aref s 3) ?c)
             (= (aref s 4) 10))))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "abc")
      (goto-char 1)
      (call-interactively 'transpose-lines)
      (let ((s (buffer-string)))
        (and (= (length s) 5)
             (= (aref s 0) 10)
             (= (aref s 1) ?a)
             (= (aref s 2) ?b)
             (= (aref s 3) ?c)
             (= (aref s 4) 10))))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "line1" (char-to-string 10) "line2")
      (goto-char 1)
      (command-execute 'transpose-lines)
      (let ((s (buffer-string)))
        (and (= (length s) 12)
             (= (aref s 5) 10)
             (= (aref s 11) 10)
             (string= (substring s 0 5) "line2")
             (string= (substring s 6 11) "line1"))))
  (error err))

(condition-case err
    (with-temp-buffer
      (insert "line1" (char-to-string 10) "line2")
      (goto-char 1)
      (call-interactively 'transpose-lines)
      (let ((s (buffer-string)))
        (and (= (length s) 12)
             (= (aref s 5) 10)
             (= (aref s 11) 10)
             (string= (substring s 0 5) "line2")
             (string= (substring s 6 11) "line1"))))
  (error err))
