(face-id 'face-id-a)
(face-id 'face-id-b)
(internal-make-lisp-face 'face-id-a)
(face-id 'face-id-a)

(internal-make-lisp-face nil)
(face-id nil)
(internal-make-lisp-face t)
(face-id t)

(condition-case err (face-id "face-id-a") (error err))
(condition-case err (face-id 'default nil nil) (error err))
