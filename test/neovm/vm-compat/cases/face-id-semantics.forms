; Lock in face-id ids, arity, type checks, and dynamic-id assignment.
(fboundp 'face-id)
(face-id 'default)
(face-id 'bold)
(face-id 'mode-line)
(face-id 'default nil)
(face-id 'default t)
(face-id 'default 'foo)
(condition-case err (face-id "default") (error err))
(condition-case err (face-id 1) (error err))
(condition-case err (face-id nil) (error err))
(condition-case err (face-id t) (error err))
(condition-case err (face-id 'unknown-face) (error err))

(internal-make-lisp-face 'face-id-a)
