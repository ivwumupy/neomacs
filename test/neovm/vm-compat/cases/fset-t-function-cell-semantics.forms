(let ((orig (symbol-function 't)))
  (unwind-protect
      (progn
        (fset t 'car)
        (funcall t '(1 2)))
    (fset 't orig)))
(let ((orig (symbol-function 't)))
  (unwind-protect
      (progn
        (fset t 'car)
        (apply t '((1 2))))
    (fset 't orig)))
(let ((orig (symbol-function 't)))
  (unwind-protect
      (progn
        (fset t nil)
        (fboundp 't))
    (fset 't orig)))
(let ((orig (symbol-function 't)))
  (unwind-protect
      (progn
        (fset t nil)
        (condition-case err (funcall t) (error err)))
    (fset 't orig)))
(let ((orig (symbol-function 't)))
  (unwind-protect
      (progn
        (fset t 1)
        (fboundp 't))
    (fset 't orig)))
(let ((orig (symbol-function 't)))
  (unwind-protect
      (progn
        (fset t 1)
        (condition-case err (funcall t) (error err)))
    (fset 't orig)))
