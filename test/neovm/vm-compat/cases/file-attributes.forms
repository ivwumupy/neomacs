; file-attributes payload shape and id-format behavior.
(setq vm-fa-file
      (let* ((forms-file (getenv "NEOVM_FORMS_FILE"))
             (base (if forms-file (file-name-directory forms-file) default-directory)))
        (expand-file-name "directory-files-fixtures/alpha.txt" base)))
(let ((a (file-attributes vm-fa-file)))
  (list (length a)
        (listp (nth 4 a)) (length (nth 4 a))
        (listp (nth 5 a)) (length (nth 5 a))
        (listp (nth 6 a)) (length (nth 6 a))
        (integerp (nth 10 a))
        (integerp (nth 11 a))))
(let ((a (file-attributes vm-fa-file t)))
  (list (stringp (nth 2 a)) (stringp (nth 3 a))))
(file-attributes "/nonexistent_path_neovm_compat_12345")
(condition-case err (file-attributes) (car err))
(condition-case err (file-attributes vm-fa-file nil nil) (car err))
